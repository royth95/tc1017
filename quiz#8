#include <vector>
#include <iostream>
using namespace std;


int sumsquares_list(vector<int>x ){
    int number=0;
    int sum=0;
    int result=0;
    for(int i=0; i<x.size(); i++){
    result=x[i]*x[i];
    sum +=result;
    }
  return sum;
  }
  int main(){
  int size=0;
  int number=0;
  cout<<"enter the size"<< endl;
  cin>> size;
  vector<int> vector1;
  for(int i=0; i<size; i++){
  cout<< "input"<<i+1<<"number"<< endl;
  cin>> number;
  vector1.push_back(number);
  }
  cout<< "the result is:"<< sumsquares_list(vector1)<< endl;
  return 0;
  }
